                        <TextBox Width="77" Margin="5,0,0,0" Text="{Binding nowsystolicpressure}"></TextBox>
                        <TextBlock Foreground="White" FontSize="16" Margin="5,0,0,0">mmHg</TextBlock>
                        <TextBlock Foreground="White" FontSize="16" Margin="40,0,0,0">低压</TextBlock>
                        <TextBox Width="77" Margin="5,0,0,0" Text="{Binding nowdiastolicpressure}"></TextBox>
