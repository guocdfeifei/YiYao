                        <RadioButton IsChecked="{Binding smoking, Converter={StaticResource yesOrNo},ConverterParameter=no}" FontSize="16" Foreground="White" Padding="0,0,0,0" VerticalContentAlignment="Bottom" Margin="50,0,0,0" >未知</RadioButton>
                        <RadioButton IsChecked="{Binding smoking, Converter={StaticResource yesOrNo},ConverterParameter=yes}" FontSize="16" Foreground="White" Padding="0,0,0,0" VerticalContentAlignment="Bottom" Margin="60,0,0,0">是</RadioButton>
                        <RadioButton IsChecked="{Binding smoking, Converter={StaticResource yesOrNo},ConverterParameter=no}" FontSize="16" Foreground="White" Padding="0,0,0,0" VerticalContentAlignment="Bottom" Margin="75,0,0,0">否</RadioButton>
                        <Image Source="/Images/Risk/required_star.png" Width="13" Height="13" Margin="20,0,0,0"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
