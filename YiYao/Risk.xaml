                        <RadioButton IsChecked="{Binding drinking, Converter={StaticResource yesOrNo},ConverterParameter=no}" FontSize="16" Foreground="White" Padding="0,0,0,0" VerticalContentAlignment="Bottom" Margin="60,0,0,0" Content="是"/>
