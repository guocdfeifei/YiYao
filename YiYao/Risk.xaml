                        <RadioButton IsChecked="{Binding eatingpreference, Converter={StaticResource yesOrNo},ConverterParameter=2}" FontSize="16" Foreground="White" Padding="0,0,0,0" VerticalContentAlignment="Bottom" Margin="60,0,0,0" Content="低盐"/>
                        <Image Source="/Images/Risk/required_star.png" Width="13" Height="13" Margin="20,0,0,0"/>

                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
