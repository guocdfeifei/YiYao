        <Canvas x:Name="static_canvas_userinfo" HorizontalAlignment="Left" Height="23" Margin="850,320,0,0" VerticalAlignment="Top" Width="132">
            <TextBlock x:Name="static_textblock_userinfo" FontSize="36" Foreground="#FF94E9FF" Width="220" Height="40"   Canvas.Top="-2" FontStretch="Condensed"  >
                <TextBlock.Text>用户基本信息</TextBlock.Text>
            </TextBlock>
        </Canvas>
        <!-- 用户信息详情：姓名 -->
        <Canvas x:Name="input_canvas_userinfo_name" HorizontalAlignment="Left" Height="23" Margin="850,400,0,0" VerticalAlignment="Top" Width="132">
            <TextBlock x:Name="input_textblock_userinfo_name" FontSize="30" Foreground="White" Width="220" Height="40"   Canvas.Top="-2" FontStretch="Condensed"  >
                <TextBlock.Text>姓名：李大力</TextBlock.Text>
            </TextBlock>
        </Canvas>
        <!-- 用户信息详情：性别 -->
        <Canvas x:Name="input_canvas_userinfo_gender" HorizontalAlignment="Left" Height="23" Margin="1150,400,0,0" VerticalAlignment="Top" Width="132">
